#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/george/Desktop/messenger/server/node_modules/.pnpm/acorn@8.8.1/node_modules/acorn/bin/node_modules:/Users/george/Desktop/messenger/server/node_modules/.pnpm/acorn@8.8.1/node_modules/acorn/node_modules:/Users/george/Desktop/messenger/server/node_modules/.pnpm/acorn@8.8.1/node_modules:/Users/george/Desktop/messenger/server/node_modules/.pnpm/node_modules:/Users/george/Desktop/messenger/server/node_modules:/Users/george/Desktop/messenger/node_modules:/Users/george/Desktop/node_modules:/Users/george/node_modules:/Users/node_modules:/node_modules:/Users/george/Desktop/messenger/server/node_modules/.pnpm/ts-node@10.9.1/node_modules/ts-node/node_modules:/Users/george/Desktop/messenger/server/node_modules/.pnpm/ts-node@10.9.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/george/Desktop/messenger/server/node_modules/.pnpm/acorn@8.8.1/node_modules/acorn/bin/node_modules:/Users/george/Desktop/messenger/server/node_modules/.pnpm/acorn@8.8.1/node_modules/acorn/node_modules:/Users/george/Desktop/messenger/server/node_modules/.pnpm/acorn@8.8.1/node_modules:/Users/george/Desktop/messenger/server/node_modules/.pnpm/node_modules:/Users/george/Desktop/messenger/server/node_modules:/Users/george/Desktop/messenger/node_modules:/Users/george/Desktop/node_modules:/Users/george/node_modules:/Users/node_modules:/node_modules:/Users/george/Desktop/messenger/server/node_modules/.pnpm/ts-node@10.9.1/node_modules/ts-node/node_modules:/Users/george/Desktop/messenger/server/node_modules/.pnpm/ts-node@10.9.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@8.8.1/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@8.8.1/node_modules/acorn/bin/acorn" "$@"
fi
